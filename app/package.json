{
  "name": "express-typescript",
  "version": "1.0.0",
  "description": "example typescript nodejs api",
  "keywords": [
    "api",
    "aws",
    "expressjs",
    "lambda",
    "nodejs",
    "serverless",
    "typescript"
  ],
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && swc ./src -d ./dist --ignore \"**/*.test.ts,**/__test__/**\"",
    "test": "jest --clearCache --coverage",
    "lint": "eslint \"src/**/*.{json,js,jsx,ts,tsx}\" && tsc --noemit",
    "prettier": "prettier \"src/**/*.{json,js,jsx,ts,tsx}\" --write"
  },
  "author": "daniele@xibitdigital.com",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.84",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "prettier": "^3.0.3",
    "rimraf": "^4.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}